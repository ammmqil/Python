def Is_prime(n):
    if n < 2:
        return False
    for i in range(2, int(n ** 0.5) + 1):
        if n % i == 0:
            return False
    return True


def max_prime_divisor(num):
    max_divisor = 1
    for i in range(2, int(num ** 0.5) + 1):
        if num % i == 0:
            #проверяем оба делителя
            if Is_prime(i):
                max_divisor = max(max_divisor, i)
            if Is_prime(num // i):
                max_divisor = max(max_divisor, num // i)
    #если число само простое
    if max_divisor == 1 and Is_prime(num):
        return num

    return max_divisor if max_divisor > 1 else None


print(max_prime_divisor(121))
