def Sort_string_one():
    text = []
    print('Введите строки')
    while True:
        word = input()
        if word == '':
            break
        text.append(word)

    #вероятности букв в английском алфавите
    freq_alphabet = {
        'e': 0.13, 't': 0.105, 'a': 0.081, 'o': 0.079, 'n': 0.071,
        'r': 0.068, 'i': 0.063, 's': 0.061, 'h': 0.052, 'd': 0.038,
        'l': 0.034, 'f': 0.029, 'c': 0.027, 'u': 0.023, 'm': 0.021,
        'p': 0.018, 'y': 0.012, 'w': 0.012, 'g': 0.011, 'b': 0.011,
        'v': 0.008, 'k': 0.003, 'x': 0.001, 'j': 0.001, 'q': 0.001,
        'z': 0.001, ' ': 0.2 }

    #разница для каждой строки
    diff = []
    for s in text:
        if len(s) == 0:
            diff.append(0)
            continue

        s_lower = s.lower()
        #самый частый символ
        max_count = 0
        most_frequent_char = ''
        for char in s_lower:
            if char.isalpha():
                count = s_lower.count(char)
                if count > max_count:
                    max_count = count
                    most_frequent_char = char

        if max_count == 0:
            diff.append(0)
            continue

        #частота в строке
        freq_in_string = max_count / len(s)

        #частота этого символа в алфавите
        if most_frequent_char in freq_alphabet:
            freq_in_alph = freq_alphabet[most_frequent_char]

        #разница
        diff.append(abs(freq_in_string - freq_in_alph))

    #сортировка
    for i in range(len(text)):
        for j in range(i + 1, len(text)):
            if diff[i] > diff[j]:
                text[i], text[j] = text[j], text[i]
                diff[i], diff[j] = diff[j], diff[i]

    for i in range(len(text)):
        print(f"{text[i]} (разница={diff[i]})")

Sort_string_one()
